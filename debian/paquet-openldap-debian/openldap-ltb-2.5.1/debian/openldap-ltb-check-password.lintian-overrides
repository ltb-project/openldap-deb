# ignore presence of configuration file in /usr
openldap-ltb-check-password binary: file-in-usr-marked-as-conffile usr/local/openldap/etc/openldap/check_password.conf
# OpenLDAP-LTB is installed in /usr/local/
openldap-ltb-check-password binary: dir-in-usr-local
openldap-ltb-check-password binary: file-in-usr-local
openldap-ltb-check-password binary: file-in-unusual-dir
